{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "maxdev00adf"
		},
		"MaxDev_databricks_ls_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0222-172249-wf0ly7k4"
		},
		"MaxDev_datalake_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://maxdev00storage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "maxdev00adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"MaxDev_databricks_ls_properties_typeProperties_existingClusterId": {
						"value": "[parameters('MaxDev_databricks_ls_properties_typeProperties_existingClusterId')]"
					},
					"MaxDev_datalake_ls_properties_typeProperties_url": {
						"value": "[parameters('MaxDev_datalake_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}